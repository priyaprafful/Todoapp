extends ./layout.pug

block prepend body
  include ./partials/header.pug

  - var pageContent = aboutusContent.data
  


  div.container
      for slice in aboutusContent.data.page_content
        case slice.slice_type
          when 'call_to_action'
            include ./partials/pageactionslice.pug
          when 'mesh_image'
            include ./partials/fullwidthimage.pug  
          when 'image_info_section'
            include ./partials/heighlighslice.pug 
          when 'email_signup'
            include ./partials/notifiedslice.pug