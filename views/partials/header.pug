header.header-fixed
  .header-limiter
    a(href='./')
      img.check-icon.light-icon(src=imageRootFolder+"/check2.png") 
      img.check-icon.dark-icon(src=imageRootFolder+"/check.png") 
      logo.logo Todoop
    if allmenuContent != null
      nav
        each menuLink in allmenuContent.data.menu_links
           a(href=PrismicDOM.Link.url(menuLink.link, ctx.linkResolver))
            != PrismicDOM.RichText.asText(menuLink.label)
        if prismicResponse
          each altlang in prismicResponse.alternate_languages
              a(href=ctx.linkResolver(altlang))
                p(class="flag-icon flag-icon-"+altlang.lang.slice(-2))
        a(href="/changeMode")
          img.light-icon(src=imageRootFolder+"/moon2.png")
          img.dark-icon(src=imageRootFolder+"/moon.png")